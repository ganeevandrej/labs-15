#include <iostream>
#include <cmath>

using namespace std;
const float pi = 3.14;
float RingS( float R1,float R2) {
    float S1 = pi * R1*R1;
    float S2 = pi * R2*R2;
    float res = abs( S1 - S2);
    return res;
}
int main()
{
    cout << RingS(3, 2) << endl;
    cout << RingS(3, 4) << endl;
    cout << RingS(4, 5) << endl;
    return 0; 
}
